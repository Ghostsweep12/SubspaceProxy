{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "Subspace Proxy",
	"version": "1.0.0-alpha",
	"identifier": "com.ghostsweep12.subspace-proxy",
	"build": {
		"beforeDevCommand": "npm run dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "npm run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "proxy-app",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"resources": ["functions.sh"],
		"externalBin": ["tun2socks"],
		"active": true,
		"targets": "all",
		"icon": [
			"icons/128x128.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
